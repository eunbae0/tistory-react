{
  "name": "@tistory-react/core",
  "version": "0.0.8-alpha.0",
  "description": "Create Tistory Skin with React.js",
  "bugs": "https://github.com/eunbae0/tistory-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eunbae0/tistory-react",
    "directory": "packages/cli/core"
  },
  "type": "module",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/node/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./theme": {
      "default": "./theme.ts"
    },
    "./theme.css": "./theme.css",
    "./runtime": {
      "default": "./runtime.ts"
    }
  },
  "scripts": {
    "dev": "modern build -w",
    "build": "modern build",
    "reset": "rimraf ./**/node_modules",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@rsbuild/core": "1.0.1-beta.2",
    "@rsbuild/plugin-react": "1.0.1-beta.4",
    "@tistory-react/runtime": "^0.0.8-alpha.0",
    "@tistory-react/shared": "^0.0.8-alpha.0",
    "@tistory-react/theme-default": "0.0.8-alpha.0",
    "deepmerge": "^4.3.1",
    "enhanced-resolve": "^5.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "rspack-plugin-virtual-module": "^0.1.13",
    "xml-js": "^1.6.11"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHead": "5955246c00aab7a0e66bc7c7626f8eef2114d96a"
