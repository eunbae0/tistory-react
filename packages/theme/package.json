{
  "name": "@tistory-react/theme-default",
  "version": "0.0.8-alpha.0",
  "description": "The Default Theme of Tistory-react Framework, Provide Tistory Components",
  "bugs": "https://github.com/eunbae0/tistory-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eunbae0/tistory-react",
    "directory": "packages/theme-default"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "imports": {
    "#theme/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "modern build -w",
    "build": "modern build",
    "reset": "rimraf ./**/node_modules",
    "test": "echo nothing"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "dependencies": {
    "@tistory-react/runtime": "^0.0.8-alpha.0",
    "@tistory-react/shared": "^0.0.8-alpha.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  },
  "sideEffects": [
    "*.css",
    "./src/styles/index.ts",
    "./dist/index.js"
  ],
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
